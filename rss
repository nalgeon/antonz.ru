    (<span style="color:#1c01ce">90</span>, <span style="color:#c41a16">&#34;ะะฐะปะตัะฐ&#34;</span>),
    (<span style="color:#1c01ce">88</span>, <span style="color:#c41a16">&#34;ะะฐัะธั&#34;</span>),
